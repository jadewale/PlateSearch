{
  "name": "Plate Search",
  "description": "Plate Search.",
  "keywords": [
    "productivity",
    "HTML5",
    "scalpel"
  ],
  "website": "https://ancient-brushlands-14749.herokuapp.com/",
  "repository": "https://github.com/jadewale/PlateSearch",
  "logo": "favicon.ico",
  "success_url": "/login",
  "scripts": {
    "postdeploy": "yarn start"
  },
  "env": {
    "SECRET_TOKEN": {
      "description": "A secret key for verifying the integrity of signed cookies.",
      "generator": "secret"
    },
    "WEB_CONCURRENCY": {
      "description": "The number of processes to run.",
      "value": "5"
    }
  },
  "formation": {
    "web": {
      "quantity": 2,
      "size": "Performance-M"
    }
  },
  "image": "node:8",
  "environments": {
    "test": {
      "scripts": {
        "test": "docker container run -it -v $(pwd):/app react:app test"
      }
    }
  }
}